{
  "name": "Jira Burndown Chart",
  "description": "A Jira Burndown Chart with Vue.js Chrome Web Extension",
  "version": null,
  "permissions": [
    "https://*/",
    "storage",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Jira Burn Chart"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
